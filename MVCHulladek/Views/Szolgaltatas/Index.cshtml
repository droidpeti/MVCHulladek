@model IEnumerable<MVCHulladek.Models.Szolgaltatas>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tipus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Jelentes)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tipus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Jelentes)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SzolgaltatasId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SzolgaltatasId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SzolgaltatasId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
